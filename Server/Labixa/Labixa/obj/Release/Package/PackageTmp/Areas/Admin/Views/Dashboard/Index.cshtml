@{
    ViewBag.Title = "Trang thống kê | KLD Website";

}
@section breadcrumb
{
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
        </script>

        <ul class="breadcrumb">
            <li>
                <a href="/Admin">Trang chủ <i class="fa fa-home" aria-hidden="true"></i></a>
            </li>
            <li class="active">Thống kê hệ thống <i class="fa fa-signal" aria-hidden="true"></i></li>
        </ul>
    </div>
}

<div class="page-header">
    <h1 style="text-align: center; margin-bottom: 20px;">
        Thống kê website
    </h1>
    <div class="row">
        <div class="col-md-12">
            <div class="widget-box">
                <div class="widget-header widget-header-flat widget-header-small">
                    <h5>
                        <i class="icon-signal"></i>
                        Lượng truy cập
                    </h5>
                </div>

                <div class="widget-body">
                    <div class="widget-main">
                        <div id="reportStaff" style="min-width: 310px; height: 400px; margin: 0 auto"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>